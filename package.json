{
  "name": "jstl",
  "version": "1.0.4",
  "displayName": "JSTL Language Support",
  "description": "JSTL Language Support adds support for JavaScript Template Literal files in Visual Studio Code",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "publisher": "jsxtools",
  "license": "CC0-1.0",
  "repository": "jsxtools/jstl-language",
  "homepage": "https://github.com/jsxtools/jstl-language#readme",
  "bugs": "https://github.com/jsxtools/jstl-language/issues",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#293845",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jstl",
        "aliases": [
          "JSTL",
          "jstl",
          "JavaScript Template Literal",
          "JS Template Literal"
        ],
        "extensions": [
          ".jstl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "source.jstl",
        "path": "./syntaxes/source.jstl.tmLanguage.json",
        "language": "jstl"
      },
      {
        "scopeName": "text.html.markdown.jstl",
        "path": "./syntaxes/text.html.markdown.jstl.injection.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.jstl": "jstl"
        }
      },
      {
        "scopeName": "source.jstl.embedded",
        "path": "./syntaxes/text.html.basic.jstl.injection.2.json",
        "injectTo": [
          "text.html"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.jstl.html": "jstl"
        }
      }
    ]
  },
  "keywords": [
    "11ty",
    "block",
    "blocks",
    "eleventy",
    "highlight",
    "highlights",
    "highlighter",
    "htm",
    "html",
    "javascript",
    "js",
    "jstl",
    "jsx",
    "language",
    "languages",
    "literal",
    "literals",
    "markdown",
    "md",
    "plugin",
    "source",
    "support",
    "supports",
    "syntax",
    "template",
    "templates",
    "templating",
    "text",
    "tl",
    "vscode"
  ]
}
